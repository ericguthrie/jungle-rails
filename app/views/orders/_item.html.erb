<tr>
  <td>
    <%= image_tag Product.find(product_id).image.tiny%>
  </td>
  <td>
    <h4><%= Product.find(product_id).name%></h4>
    <br>
    <p><%= Product.find(product_id).description%></p>
  </td>
  <td>
    
    <%=humanized_money_with_symbol Product.find(product_id).price%>
  </td>
  <td>
    <%= quantity %>
  </td>
  <td>
    
    <%= humanized_money_with_symbol  total_price_cents/100 %>
  </td>
  
</tr>